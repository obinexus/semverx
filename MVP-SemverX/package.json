{
  "name": "@obinexus/registry",
  "version": "1.0.0",
  "description": "r.obinexus.org Registry Client with SemVerX support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "obinexus-registry": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "semverx",
    "package-manager",
    "dependency-resolution",
    "dag",
    "avl-tree",
    "obinexus",
    "registry",
    "polyglot"
  ],
  "author": "OBINexus <support@obinexus.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/obinexus/registry-client-node.git"
  },
  "homepage": "https://r.obinexus.org",
  "bugs": {
    "url": "https://github.com/obinexus/registry-client-node/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "semverx": {
    "registry": "https://r.obinexus.org",
    "policy": {
      "allow_experimental": false,
      "auto_update": "opt-in",
      "max_update_rate": 10
    },
    "resolution_strategy": "hybrid",
    "fault_tolerance": {
      "max_panic_level": 11,
      "auto_rollback": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
